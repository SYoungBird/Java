package day0316;

public class Pg17 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i=5, j;
		i=i++;
		System.out.println("j="+i);
		i=++i;
		System.out.println("j="+i);
		i=i--;
		System.out.println("j="+i);
		i=--i;
		System.out.println("j="+i);
		
		
		System.out.println("ÇöÀç i="+i);
		System.out.println("i++ : "+i++);
		System.out.println("++i : "+(++i));
		System.out.println("i-- : "+i--);
		System.out.println("--i : "+(--i));
	}

}
