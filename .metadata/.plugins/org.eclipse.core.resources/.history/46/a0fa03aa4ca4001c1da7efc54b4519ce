package OP;

public class op2 {

	public static void main(String[] args) {
		//비트연산자 : &, |, ^
		// & : 자리수끼리 비교해서 모두 1이면 1
		// | : 자리수끼리 비교해서 하나라도 1이면 1
		// ^ : 자리수끼리 비교해서 서로 다르면 1
		System.out.println("3&5는 "+(3&5));
		System.out.println("3|5는 "+(3|5));
		System.out.println("3^5는 "+(3^5)));
	}

}
